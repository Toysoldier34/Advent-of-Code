var myArray:Array = parString.split("");
trace("Start");
for (var i:int = 0; i < myArray.length; i++) {
	if (myArray[i] == "(") {
		floor +=  1;
	} else if (myArray[i] == ")") {
		floor -=  1;
	} else {
		trace("error");
	}
	if (floor == -1){
		trace(i + 1);
		break;
	}
}
trace(floor);
